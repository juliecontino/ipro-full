<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Port_Type_Matches_Access_Type</fullName>
    <active>true</active>
    <description>Port Type Matches Access Type</description>
    <errorConditionFormula>TEXT(Port_Type__c) &lt;&gt; TEXT(Access_Type__c)</errorConditionFormula>
    <errorDisplayField>Access_Type__c</errorDisplayField>
    <errorMessage>The Port Type and the Access Type must be the same.</errorMessage>
</ValidationRule>
