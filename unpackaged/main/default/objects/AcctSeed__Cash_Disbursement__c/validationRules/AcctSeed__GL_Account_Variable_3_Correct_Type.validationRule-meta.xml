<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AcctSeed__GL_Account_Variable_3_Correct_Type</fullName>
    <active>true</active>
    <description>Ensures the GL Variable 4 field is of type GL Variable 3</description>
    <errorConditionFormula>NOT(ISBLANK( AcctSeed__GL_Account_Variable_3__c )) &amp;&amp; TEXT(AcctSeed__GL_Account_Variable_3__r.AcctSeed__Type__c) != "GL Account Variable 3"</errorConditionFormula>
    <errorDisplayField>AcctSeed__GL_Account_Variable_3__c</errorDisplayField>
    <errorMessage>The GL Variable must have a Type of "GL Account Variable 3".</errorMessage>
</ValidationRule>
