<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>JCSoftware__Integer_Validation</fullName>
    <active>true</active>
    <description>The "Integer_Suffix" default value has to be a number.</description>
    <errorConditionFormula>JCSoftware__Behavior_Rule__c  = "Integer_Suffix" &amp;&amp;
ISNUMBER(JCSoftware__Behavior_Value__c) = FALSE</errorConditionFormula>
    <errorDisplayField>JCSoftware__Behavior_Value__c</errorDisplayField>
    <errorMessage>The default value for the "Integer_Suffix" behavior must be an integer.</errorMessage>
</ValidationRule>
