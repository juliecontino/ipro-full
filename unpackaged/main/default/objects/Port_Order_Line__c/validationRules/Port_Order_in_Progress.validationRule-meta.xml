<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Port_Order_in_Progress</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISPICKVAL( Port_Order__r.Status__c,"New")))</errorConditionFormula>
    <errorMessage>The Port Order Status is already is Progress or Completed.  It must be a "New" Status</errorMessage>
</ValidationRule>
