<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FLAGS__Case_Flag_Business_Time__c</fullName>
    <deprecated>false</deprecated>
    <description>Renders the Case Flag in business hours as "X Days Y Hours Z Minutes".</description>
    <externalId>false</externalId>
    <formula>IF(ISNULL(FLAGS__ViewedFlag__c),null,
FLAGS__Case_Flag_Business_Time_Days__c + ' ' +
FLAGS__Case_Flag_Business_Time_Hours__c + ' ' +
FLAGS__Case_Flag_Business_Time_Mins__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Case Flag Business Time</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
