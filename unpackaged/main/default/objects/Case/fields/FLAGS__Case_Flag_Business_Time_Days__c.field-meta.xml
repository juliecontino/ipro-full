<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FLAGS__Case_Flag_Business_Time_Days__c</fullName>
    <deprecated>false</deprecated>
    <description>Renders the day portion of the Case Flag in business hours as "X Days".</description>
    <externalId>false</externalId>
    <formula>IF(FLAGS__Case_Flag_Business_Age__c &lt; 24, null,
TEXT(ROUND(FLAGS__Case_Flag_Business_Age__c /24-0.49999,0)) + ' Days')</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Case Flag Business Time Days</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
