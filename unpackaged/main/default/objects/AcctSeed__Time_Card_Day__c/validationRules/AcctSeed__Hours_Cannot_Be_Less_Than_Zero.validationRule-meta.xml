<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AcctSeed__Hours_Cannot_Be_Less_Than_Zero</fullName>
    <active>true</active>
    <description>The hours field cannot be a negative number</description>
    <errorConditionFormula>AcctSeed__Hours__c &lt; 0</errorConditionFormula>
    <errorDisplayField>AcctSeed__Hours__c</errorDisplayField>
    <errorMessage>You cannot specify a negative number for hours.</errorMessage>
</ValidationRule>
