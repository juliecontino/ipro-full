<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AcctSeedERP__Executed_Average_Cost__c</fullName>
    <deprecated>false</deprecated>
    <formula>IF(ISBLANK(AcctSeedERP__Prior_Average_Cost__c), NULL, ((AcctSeedERP__Prior_Average_Cost__c * AcctSeedERP__Prior_Available_Quantity__c) + (AcctSeedERP__Quantity__c *  AcctSeedERP__Unit_Cost__c ))/ (AcctSeedERP__Prior_Available_Quantity__c +  AcctSeedERP__Quantity__c))</formula>
    <inlineHelpText>The average cost of this product for this ledger including this transaction.</inlineHelpText>
    <label>Executed Average Cost</label>
    <precision>18</precision>
    <required>false</required>
    <scale>6</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
