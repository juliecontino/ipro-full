                                    <option value="All">All</option>
                                    <option value="DiscoverOrg ID">DiscoverOrg ID</option>
                                    <option value="Contact Email">Contact Email</option>
                                </select>
                            </div>
                                    
                            <div  id="buttons" role="group" >
                                <!--Contact Header-->
                                <apex:outputPanel layout="none" rendered="{!!isAccountManager && !isForLead }">
                                    <apex:actionFunction name="goToPageFunContact" action="{!goToPageFun}" reRender="frm1,parentPanel" oncomplete="saveContactAll();overrideBtnAndMsgStyle();makeSelectOption();"/> 
                                    <apex:actionFunction name="saveContactAll" action="{!SaveContactsAll}" reRender="frm1,parentPanel" oncomplete="saveAllContactNNext();overrideBtnAndMsgStyle();makeSelectOption();"/>          
                                    <script>            
                                        function saveAllContactNNext(){                                        
                                            showStatus();
                                            if( startPage <= {!totalpage} ){
                                                $('input[id*=pageNoId]').val(startPage);
                                                goToPageFunContact();
                                                startPage++;
                                            }else{
                                                hideStatus();
                                            }                                       
                                        }
                                        
                                        function doFilter(filterSelVal){            
                                            if( filterSelVal == 'DiscoverOrg ID' ){
                                                showOnlyByID();
                                            }
                                            else if( filterSelVal == 'Contact Email' ){
                                                showOnlyByEmails();
                                            }
                                            else{
                                                showByAll();
                                            }
                                            selectedFilter = filterSelVal;
                                        }
                                        
                                        function pickOnChange(){
                                            doFilter( $("#fltrOpt").val() );
                                        }
                                    </script>                                                                        
                                    
                                    <apex:commandButton action="{!SaveContacts}"  oncomplete="hideStatus();" onclick="showStatus()" styleClass="slds-button slds-button--brand" reRender="parentPanel,frm1" value="Update Selected" style="font-size: 14px;min-width: 140px;font-weight: 500;" disabled="{!If(AND(NOT(ISNULL(result.multiDups)),result.multiDups.size>0),'false','true')}"/>
                                    <apex:commandButton onclick="saveAllContactNNext();return false;" styleClass="slds-button slds-button--brand" value="Update All" style="font-size: 14px;min-width: 140px;font-weight: 500;" reRender="frm1,parentPanel" disabled="{!If(AND(NOT(ISNULL(result.multiDups)),result.multiDups.size>0),'false','true')}"/>                                                                                                      
