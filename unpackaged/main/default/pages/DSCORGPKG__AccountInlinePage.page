                        $( ".techDescription" ).each(function( index ) {
                            var originalBody = '';          
                            var htBody = '';
                            var addLink = '';
                            var checkBody = '';
                            
                            //If data have See More
                            if( $(this).attr("short-data-value-link") != '' ){
                                var fulVal = $(this).attr("full-data-value");
                                
                                //Regex
                                var reg2 = new RegExp('class="highlight"', 'gi');
                                var reg = new RegExp(toSearch, 'gi');
                                
                                
                                //Marking
                                
                                var headLnk = $(this).parent().find(".tectHeader").attr("data");
                                
                                headLnk = headLnk.replace(reg, function(str) {return '<span class="highlight">'+str+'</span>'});
                                fulVal = fulVal.replace(reg, function(str) {return '<span class="highlight">'+str+'</span>'});
                                
                                //Checking where we find data. Is in full or short
                                var fullCount = (fulVal.match(reg2) || []).length;
                                var headerCount = (headLnk.match(reg2) || []).length;
                                
                                if( fullCount+headerCount > 0 ){
                                    addLink ='';
                                    $(this).parent().find(".tectHeader").html(headLnk);
                                    htBody = fulVal; 
                                }
                                else{
                                    $(this).parent().hide();
                                }
                                
                                htBody += ' '+addLink;   
                                $(this).html( htBody );
                                
                                if( toSearch == '' ){
                                    var shortVal = $(this).attr("short-data-value");
                                    $(this).html( shortVal + ' ' +"<a href='javascript:void(0);' onclick='seeMore(this); return false' class='see-more'>See More...</a>" );
