                    $( "[data-apiname*='IT_Budget__c']").next().html( nFormatter(itBug) );
                }
                catch(err) {}
            }
            
            function nFormatter(num) {
                if (num >= 1000000) {
                    return (num / 1000000).toFixed(1).replace(/\.0$/, '') + ' M';
                }
                return num;
            }
            
            
            
