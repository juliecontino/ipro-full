                                    function decodeHtml(html) {
                                        var txt = document.createElement("textarea");
                                        txt.innerHTML = html;
                                        return txt.value;
                                    }
                                    
                                    function Removehilight(ele) {
                                      var sbody = $(ele).html();
                                      sbody = decodeHtml(sbody);
                                      sbody = sbody.replace(/<font class="highlight">([^<]+)<\/font>/gmi, '$1');  // remove previous hilighted text
                                      $(ele).html(sbody);
                                    }
                                    
                                    function hilight(ele, search) {
                                      var sbody = $(ele).html();
                                      sbody = decodeHtml(sbody);
                                      var re = new RegExp('(' + search + ')(?![^<]*>|[^<>]*</)', 'gim');
        
                                      subst = '<font class="highlight">$1</font>';
                                      var result = sbody.replace(re, subst);
                                      $(ele).html(result);
                                    }
                                    
