                            <script>
                                function doTriggerGlobalSearch(){
                                    //Show All
                                    $(".DescriptionTD").parent().show();
                                    $(".DescriptionTD").parent().prev().show();
                                
                                    //Date Filter Start here
                                    var selM = $('#filterbymonth .thisistick').text();
                                    if( selM != '' && selM.toLowerCase().indexOf('all') ==-1 ){
                                        var monInt = parseInt(selM.split(' ')[0]);
                                        var selDate = new Date();
                                        selDate.setMonth(selDate.getMonth() - monInt);
                                        
                                        $(".testcs").each(function() {
                                            var tddate = $(this).text();
                                            var tdmon = tddate.split("/");
                                            var tdDate = new Date();
                                            tdDate.setYear(parseInt(tdmon[2]));
                                            tdDate.setMonth(parseInt(tdmon[0]));
                                            tdDate.setDate(parseInt(tdmon[1]));
                                            
                                            if( selDate >= tdDate ){
                                                $(this).parent().parent().hide();
                                                $(this).parent().parent().prev().hide();
                                            }
                                        });
                                    }
                                    //Date Filter Start End
                                    
                                    //Category Filter Starts Here
                                    var selCat = '';
                                    $(".Categorylist").each(function() {
                                        if( $(this).hasClass("thisistick") ){
                                            selCat = $(this).text().trim().toLowerCase();
                                        }
                                    });
                                    if( selCat != '' && selCat.toLowerCase().indexOf('all') ==-1 ){           
                                        $(".DescriptionTD").each(function() {       
                                            var anyFound = false;                          
                                            $(this).find(".Category").each(function() {
                                                if( $(this).text().trim().toLowerCase().indexOf(selCat) != -1 && $(this).parent().parent().parent().css("display") != 'none' ){
                                                    anyFound = true;
                                                }
                                            });
                                            if( anyFound == false ){
                                                $(this).parent().hide();
                                                $(this).parent().prev().hide();
                                            }
                                        });
                                    }
                                    //Category Filter Ends Here
                                    
                                    //Text Search Start Here
                                    var toSearch = $(".trigrSearchBox").val();
                                    $( ".DescriptionTD" ).each(function( index ) {
                                        Removehilight( $(this) );
                                    });
                                    if( toSearch != '' ){
                                        $( ".DescriptionTD" ).each(function( index ) {
                                            if( $(this).parent().css("display") != 'none' ){
                                            
                                                hilight($(this) , toSearch);
                                                
                                                $(this).find( ".Category" ).each(function( index ) {
                                                    hilight($(this) , toSearch);
                                                });
                                                
                                                $(this).find("a").each(function( index ) {
                                                    hilight($(this) , toSearch);
                                                });
                                            }
                                        });
                                        
                                        $( ".DescriptionTD" ).each(function( index ) {
                                            if( $(this).parent().css("display") != 'none' ){
                                                var htData = $(this).html();
                                                var reg2 = new RegExp('class="highlight"', 'gi');
                                                var foundCount = (htData.match(reg2) || []).length;
                                                if( foundCount == 0 ){
                                                    $(this).parent().hide();
                                                    $(this).parent().prev().hide();
                                                }
                                            }
                                        });
                                    }
                                    
                                    if( toSearch == '' ){
                                        $("#crossSrchIconTrig").hide();
                                    }
                                    else{
                                        $("#crossSrchIconTrig").show();
                                    }
                                    //Text Search End Here
                                    
                                    var totTri = 0;
                                    $(".rtt-items").find(".slds-hint-parent").each(function( index ) {
                                        if( $(this).css("display") != 'none'  ){
                                            totTri = totTri+1;
                                        }
                                    });
                                    $('.displaytrigger').html('Displaying '+totTri+' Scoops');
                                    
                                }
                                
                                function decodeHtml(html) {
                                    var txt = document.createElement("textarea");
                                    txt.innerHTML = html;
                                    return txt.value;
                                }
                                
                                function Removehilight(ele) {
                                  var sbody = $(ele).html();
                                  sbody = decodeHtml(sbody);
                                  sbody = sbody.replace(/<font class="highlight">([^<]+)<\/font>/gmi, '$1');  // remove previous hilighted text
                                  $(ele).html(sbody);
                                }
                                
                                function hilight(ele, search) {
                                  var sbody = $(ele).html();
                                  sbody = decodeHtml(sbody);
                                  var re = new RegExp('(' + search + ')(?![^<]*>|[^<>]*</)', 'gim');
    
                                  subst = '<font class="highlight">$1</font>';
                                  var result = sbody.replace(re, subst);
                                  $(ele).html(result);
                                }
                                
                                function doHigh2(){
                                    doTriggerGlobalSearch()
                                }
                            
                                function showli(text){
                                    text = text.toLowerCase();
                                    $(".Categorylist").each(function( index ) {
                                        if( $(this).text().trim().toLowerCase().indexOf(text) != -1 ){
                                            $(this).show();
                                        }
                                        else{
                                            $(this).hide();
                                        }
                                    });
                                }
