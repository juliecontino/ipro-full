                    $("#tableHeader > tr:first > th").eq(index).css("min-width", maxWidth+"px");
                    $("#tableHeader > tr:first > th").eq(index).css("max-width", maxWidth+"px");
                    $(this).css("min-width", maxWidth+"px");
                    $(this).css("width", maxWidth+"px");
