                //Column Width Setting
                var i = 1;
                $(firstTR).children(' td').each(function(index){

                    var eleWidth = $(this).outerWidth();
                    var hEleWidth = $("#tableHeader > tr:first > th").eq(index).outerWidth();
                    //
                    var maxWidth = eleWidth;
                    if( hEleWidth > eleWidth ){
                        maxWidth = hEleWidth;
                    }
            
                    if( $("#tableHeader > tr:first > th").eq(index).find(".fa-sort").length > 0 ){
                        maxWidth = maxWidth+12;
                    }
                    if( $("#tableHeader > tr:first > th").eq(index).find(".fa-search").length > 0 ){
