          $( function() {
        
              function runEffect(param) {
                  var selectedEffect = $( "#effectTypes" ).val();
                  var options = {};
                  if ( selectedEffect === "scale" ) {
                    options = { percent: 50 };
                  } else if ( selectedEffect === "size" ) {
                    options = { to: { width: 200, height: 60 } };
                  }  
                  
                  if(param == 'button'){
                      $( "#effect" ).toggle( 'blind', options, 200 );
                  }
                  else if(param == 'buttonTrigger'){
                      $( "#effectTrigger" ).toggle( 'blind', options, 200 );
                  }
                  setTimeout(function(){
                    adjustRTTHeight();
                  }, 500);
              }
            
            
              $( "#button" ).on( "click", function() {
                  runEffect('button');
                  if($( "#button" ).html() == 'Show Filters'){
                      $( "#button" ).html('Hide Filters');
                  }else{
                      $("#button" ).html('Show Filters');
                  }
                  return false;
              });
        
              $( "#buttonTrigger" ).on( "click", function() {
                  runEffect('buttonTrigger');
                  if($( "#buttonTrigger" ).html() == 'Show Filters'){
                      $( "#buttonTrigger" ).html('Hide Filters');
                  }else{
                      $("#buttonTrigger" ).html('Show Filters');
                  }
                  return false;
              });
          });

