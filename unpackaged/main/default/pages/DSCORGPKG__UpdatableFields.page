                }
             });
             
             function convertCheckBoxToLDS() {
                var Row = document.getElementsByClassName("convertToLSD");
                for (var k = 0; k < Row.length; k++) {
                    if ($(Row[k]).hasClass("converted") == false) {
                        var colTds = Row[k].getElementsByTagName("td");
                        for (var i = 0; i < colTds.length; i++) {
                            var inrHtml = colTds[i].innerHTML;
                            var chkId = inrHtml.substring(inrHtml.indexOf("id=") + 4, inrHtml.indexOf("\"", inrHtml.indexOf("id=") + 4));
                            var chkBx = inrHtml.substring(inrHtml.indexOf("<input"), inrHtml.indexOf(">") + 1);
                            var chkLable = colTds[i].getElementsByTagName("label")[0].textContent;
                            var typeOfInput = colTds[i].getElementsByTagName("input")[0].getAttribute("type");
                            var newChkBox = '<label class="slds-' + typeOfInput + '" for="' + chkId + '">' + chkBx +
                                '<span class="slds-' + typeOfInput + '--faux"></span>' +
                                '<span class="slds-form-element__label">&nbsp;' + chkLable.trim() + '</span>' +
                                '</label>';
                            colTds[i].innerHTML = newChkBox;
                        }
                    }
                }
                $(".convertToLSD").addClass("converted");
            }
            
            function onCompleteSaveScoopsAndOppAlert(){
                convertCheckBoxToLDS();
                document.getElementById('divProcessing').style.display='none';
            }
            
            function callObjectSection(){
                $("#objectsectionid").css("display","block");
                $("#fieldsectionid").css("display","none");
                $("#datasectionid").css("display","none");
                $("#generalsectionid").css("display","none");
                $("#dataServiceButton").css("display","none");
                $(".objectTabDiv").addClass( "addStyleOnCurrentTab" );
                $(".fieldTabDiv").removeClass( "addStyleOnCurrentTab" );
                $(".dataTabDiv").removeClass( "addStyleOnCurrentTab" );
                $(".generalTabDiv").removeClass( "addStyleOnCurrentTab" );   
                $("[id*='currentTabId']").val('objectsectionid');
            } 
            function callFieldSection(){
                $("#objectsectionid").css("display","none");
                $("#fieldsectionid").css("display","block");
                $("#datasectionid").css("display","none");
                $("#generalsectionid").css("display","none");
                $("#dataServiceButton").css("display","none");
                $(".objectTabDiv").removeClass( "addStyleOnCurrentTab" );
                $(".fieldTabDiv").addClass( "addStyleOnCurrentTab" );
                $(".dataTabDiv").removeClass( "addStyleOnCurrentTab" );
                $(".generalTabDiv").removeClass( "addStyleOnCurrentTab" );
                $("[id*='currentTabId']").val('fieldsectionid');  
            } 
            function callDataSection(){
                $("#objectsectionid").css("display","none");
                $("#fieldsectionid").css("display","none");
                $("#datasectionid").css("display","block");
                $("#generalsectionid").css("display","none");
                $("#dataServiceButton").css("display","block");
                $(".objectTabDiv").removeClass( "addStyleOnCurrentTab" );
                $(".fieldTabDiv").removeClass( "addStyleOnCurrentTab" );
                $(".dataTabDiv").addClass( "addStyleOnCurrentTab" );
                $(".generalTabDiv").removeClass( "addStyleOnCurrentTab" );
                $("[id*='currentTabId']").val('datasectionid');
            } 
            function callGeneralSection(){
                $("#objectsectionid").css("display","none");
                $("#fieldsectionid").css("display","none");
                $("#datasectionid").css("display","none");
                $("#generalsectionid").css("display","block");
                $("#dataServiceButton").css("display","none");
                $(".objectTabDiv").removeClass( "addStyleOnCurrentTab" );
                $(".fieldTabDiv").removeClass( "addStyleOnCurrentTab" );
                $(".dataTabDiv").removeClass( "addStyleOnCurrentTab" );
                $(".generalTabDiv").addClass( "addStyleOnCurrentTab" );
                $("[id*='currentTabId']").val('generalsectionid');
            }     
           
