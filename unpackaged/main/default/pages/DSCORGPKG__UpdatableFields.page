        <script type="text/javascript">           
            $(document).ready(function() {
                $('input[type=text], textarea').keyup(function(event){
                    if($(this).val())
                        $(this).val(html2text($(this).val())); 
                });             
                $('input[type=text], textarea').keydown(function(event){
                    if($(this).val())
                        $(this).val(html2text($(this).val())); 
                });
    
                var radioValue = '{!GodObject.scheduleDeleteHistory__c}';
                if(document.getElementById(radioValue) != null)
                    document.getElementById(radioValue).setAttribute('checked','checked');
                
                var radioValueConflict = '{!GodObject.scheduleDeleteConflict__c}';
                if(document.getElementById(radioValueConflict+'Conflict') != null)
                    document.getElementById(radioValueConflict+'Conflict').setAttribute('checked','checked');
                                
                $("[id*='contactsinpage']").change(function() {
                    if ($(this).is(':checked'))
                        $("[id*='contactField']").prop("disabled", false);
                    else
                        $("[id*='contactField']").prop("disabled", true);
                });
                $("[id*='accountsinpage']").change(function() {
                    if ($(this).is(':checked'))
                        $("[id*='accountField']").prop("disabled", false);
                    else
                        $("[id*='accountField']").prop("disabled", true);
                });

                $("[id*='leadinpage']").change(function() {
                    if ($(this).is(':checked'))
                        $("[id*='leadField']").prop("disabled", false);
                    else
                        $("[id*='leadField']").prop("disabled", true);
                });                
                function html2text(html){
                    var retHtml = html;
                    if( retHtml.includes('<') && retHtml.includes('>') ){
                        retHtml = '';
                    }
                    return retHtml;
                }

                if({!!isEnableAutomaticUpdates}){
                    $(".nightlyBatchCls").attr("disabled", true);
                }
                
                convertCheckBoxToLDS();
                if(window.location.href.indexOf("SelectedTab=GeneralTab") > -1) {                    
                    callGeneralSection();
                    $(".conflict-card > .slds-card__header").find(".toggleIcon").toggleClass("slds-hide").toggleClass("slds-show");
                    $(".conflict-card > .slds-card__header").next(".slds-card__body").slideToggle();
                    $(".conflict-card > .slds-card__header").toggleClass("open-header");                    
                }else if(window.location.href.indexOf("SelectedTab=ObjectTab") > -1){
                    callObjectSection();
                } else {
                    if($("[id*='currentTabId']").val() == ''){
                        $("#objectsectionid").css('display','block');
                        $("#fieldsectionid").css('display','none');
                        $("#datasectionid").css('display','none');
                        $("#generalsectionid").css('display','none');
                        $(".objectTabDiv").addClass( "addStyleOnCurrentTab" ); 
                    }else{
                        if($("[id*='currentTabId']").val() == 'datasectionid'){
                            callDataSection();                                                    
                        }else if($("[id*='currentTabId']").val() == 'generalsectionid'){
                            callGeneralSection(); 
                        }else if($("[id*='currentTabId']").val() == 'objectsectionid'){
                            callObjectSection();
                        }else if($("[id*='currentTabId']").val() == 'fieldsectionid'){
                            callFieldSection();
                        }
                        var currentTabId = '#' + $("[id*='currentTabId']").val().toString();
                        $(currentTabId).css('display','block');                    
                    }    
                }                 
                addRemoveActiveClass();
                if({!updataDataServiceOnLoad}){
                    document.getElementById('divProcessing').style.display='block';
                    updateDataServiceWithSchedular();       
                }
                if( {!!GodObject.isPrivacySettingsUpdated__c} )
                    updatePrivacySettings();
            });
            
            
